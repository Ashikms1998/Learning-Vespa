<!-- 

This file tells Vespa: These are the Docker containers where services should run.
<host name="localhost"> The real hostname of your machine or Docker container. For local development, use "localhost"
<alias>vespa-container</alias> The nickname you used in services.xml: <node hostalias="vespa-container"/>
So this is saying: Hey Vespa, when I say vespa-container, I mean the host named localhost
 -->


<hosts>
    <host name="localhost">
        <alias>vespa-container</alias>
    </host>
</hosts>
